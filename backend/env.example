# Backend environment example
PORT=3000

# Required for Agora token generation
AGORA_APP_ID=your_agora_app_id
AGORA_APP_CERTIFICATE=your_agora_app_certificate

# Frontend base URL used to generate shareable links
# e.g., https://your-frontend-ngrok.ngrok-free.app
FRONTEND_BASE_URL=http://localhost:3001

# Comma-separated additional allowed CORS origins
# e.g., https://your-frontend-ngrok.ngrok-free.app, http://localhost:3001
ALLOWED_ORIGINS=

